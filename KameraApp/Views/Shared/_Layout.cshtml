                    @{
                        var tree = ViewBag.OrganizationTree as List<KameraApp.Models.Organization>;
                        var children = (tree != null && tree.Any())
                        ? tree.FirstOrDefault()?.Children ?? new List<KameraApp.Models.Organization>()
                        : new List<KameraApp.Models.Organization>();
                    }

                    <ul class="metismenu list-unstyled" id="side-menu">
                        @RenderMenu(children)
                    </ul>



                    @functions {
                        private IHtmlContent RenderMenu(List<KameraApp.Models.Organization> items)
                        {
                            var ul = new TagBuilder("ul");
                            ul.AddCssClass("sub-menu");

                            foreach (var item in items)
                            {
                                var li = new TagBuilder("li");

                                var a = new TagBuilder("a");
                                a.Attributes["href"] = "javascript:void(0);";
                                a.AddCssClass("has-arrow");
                                a.InnerHtml.AppendHtml($"<i data-feather=\"folder\"></i> <span>{item.Name}</span>");
                                li.InnerHtml.AppendHtml(a);

                                var innerUl = new TagBuilder("ul");
                                innerUl.AddCssClass("sub-menu");

                                // Alt organizasyonları ekle
                                if (item.Children != null && item.Children.Any())
                                {
                                    innerUl.InnerHtml.AppendHtml(RenderMenu(item.Children));
                                }

                                // Bu organizasyona bağlı cihaz isimlerini ekle
                                if (item.MatchedDevices != null && item.MatchedDevices.Any())
                                {
                                    foreach (var device in item.MatchedDevices)
                                    {
                                        var deviceLi = new TagBuilder("li");
                                        deviceLi.InnerHtml.AppendHtml($@"
                    <a href=""/Camera/ShowCamera?deviceCode={device.DeviceCode}"">
                    <i data-feather=""video""></i> {device.DeviceName}
                    </a>");
                                        innerUl.InnerHtml.AppendHtml(deviceLi);
                                    }
                                }

                                if (innerUl.InnerHtml.ToString().Length > 0)
                                    li.InnerHtml.AppendHtml(innerUl);

                                ul.InnerHtml.AppendHtml(li);
                            }

                            return ul;
                        }
                    }




